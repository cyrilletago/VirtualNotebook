<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cyrille.virtualnotebook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.cyrille.virtualnotebook.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.cyrille.virtualnotebook.DisplayDataListView_old"
            android:label="@string/title_activity_showdata_listview"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.cyrille.virtualnotebook.WordDetail"></activity>
        <activity android:name="com.example.cyrille.virtualnotebook.Test"></activity>
    </application>

</manifest>